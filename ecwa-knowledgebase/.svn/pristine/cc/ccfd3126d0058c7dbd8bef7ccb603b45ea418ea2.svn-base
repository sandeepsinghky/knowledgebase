#header {
	border-bottom: 1px solid #032FB5;
	border-top: 0.5px solid #032FB5;
	/* background: url(../images/banner4.png) no-repeat top left; */
	background: #092940  no-repeat top left;
	margin: 0;
}

#logo {
  background-image: url(../images/bannerlogo.png);
  padding: 80px 130px 1px;
  display:inline-block;
  margin: 0;
}

#header h1 {
	margin: 0;
	padding: 73px 25px 1px;
	font-family: "TransportNew", Georgia, serif;
	font-size: 150%;
	text-align: center;
	color: #fff;
	/*background: url(../image/header_img_02.gif) no-repeat top left;*/
}

#pheader {
	border-bottom: 2px solid #032FB5;
	border-top: 0.5px solid #032FB5;
	background: url(../images/pbanner.png) no-repeat top left;
}

#pheader h1 {
	margin: 0;
	padding: 60px 25px 1px;
	font-family: "TransportNew", Georgia, serif;
	font-size: 150%;
	text-align: center;
	color: #374C0E;
	/*background: url(../image/header_img_02.gif) no-repeat top left;*/
}

.header2 {
	float: right;
	width: 100%;
	clear: both;
	position: relative;
	overflow: hidden;
	margin: 0;	
	background: #fff   repeat-x top left;
}

.header2 h3 {
	margin: 0;
	padding: 10px 25px 1px;
	font-family: "TransportNew", Georgia, serif;
	font-size: 150%;
	text-align: center;
	color: #374C0E;
}


#wrap {
	min-width: 500px;
	max-width: 1400px;
	background: url(../images/new-wrap-bg.gif) repeat-y 70% 0;
}

#wrap2 {
	min-width: 500px;
	max-width: 1400px;
	background: url(../images/new-wrap-plane.gif) repeat-y 70% 0;
}

#wrap3 {
	min-width: 500px;
	max-width: 1400px;
	background: url(../images/new-wrap3-bg.gif) repeat-y 70% 0;
}

.tbar {
	float: right;
	/*width: 90.5%;8/
	clear: both;
	position: relative;
	overflow: hidden;
	margin: 0;
	/*padding: 0 .5em 0 5em;*/
	list-style: none;

	/*background: #0000ff repeat-x top left; 
	 background-color: #0000ff;*/
}

.tbar li {
	float: right;
	margin: 0 2px 0 0;
	padding: 1em 0;
	font-family: "TransportNew","Lucida Grande", sans-serif;
	font-size: 13px;
	color: #f2f2f2;
}

.tbar a {
	float: right;
	display: block;
	margin: 0;
	padding: .5em 5em;
	color: #f2f2f2;
	text-decoration: underline;
	border-bottom: none;
}

/* navigation */
.nav {
	float: right;
	width: 100%;
	clear: both;
	position: relative;
	overflow: hidden;
	margin: 0;
	padding: 1em .5em 0 5em;
	list-style: none;
	/*background: #0000ff repeat-x top left;*/
	background: #336c99   repeat-x top left;
	/*background-color: #0000ff;*/
}

.nav li {
	float: left;
	margin: 0 2px 0 0;
	padding: 0 5.5em 0 0;
	font-family: "Lucida Grande", sans-serif;
	font-size: 17px;
	color: #f2f2f2;
}

.nav a {
	float: left;
	display: block;
	margin: 0;
	padding: .5em 5em;
	color: #f2f2f2;
	text-decoration: underline;
	border-bottom: none;
}

