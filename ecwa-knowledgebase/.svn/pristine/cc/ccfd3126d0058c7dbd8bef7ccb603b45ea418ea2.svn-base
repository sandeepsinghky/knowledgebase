body {
	margin: 0;
	padding: 0;
	font-family: "TransportNewHeavy", Verdana, sans-serif;
	font-size: small;
	background: #fff;
}

#content {
	float: left;
	width: 100%;
	font-size: 95%;
	color: #333;
	line-height: 1.5em;
}

#content h2 {
	margin: 0 0 1em 0;
	padding: 0;
	font-family: "TransportNewHeavy", Georgia, serif;
	font-size: 150%;
	color: #663;
	font-weight: normal;
}

#content .gutter {
	padding: 25px;
	background: url(../images/content-bg.png) repeat-x top left;
}

#aplContent {
	float: left;
	width: 100%;
	font-size: 95%;
	color: #333;
	line-height: 1.5em;
}

#aplContent h3 {
	margin: 0 0 1em 0;
	padding: 0;
	font-family: Georgia, serif;
	font-size: 150%;
	color: #663;
	font-weight: normal;
}

#aplContent .gutter2 {
	padding: 25px;
	background: url(../images/content-bg.png) repeat-x top left;
}

#contentTwo {
	float: left;
	width: 100%;
	font-size: 95%;
	color: #333;
	line-height: 1.5em;
}

#contentTwo h3 {
	margin: 0 0 1em 0;
	padding: 0;
	font-family: Georgia, serif;
	font-size: 150%;
	color: #663;
	font-weight: normal;
}

#contentTwo .gutter2 {
	padding: 25px;
	background: url(../images/content-bg.png) repeat-x top left;
}

#contentThree {
	float: right;
	width: 70%;
	font-size: 95%;
	color: #333;
	line-height: 1.5em;
}

#contentThree h4 {
	margin: 0 0 1em 0;
	padding: 0;
	font-family: Georgia, serif;
	font-size: 150%;
	color: #663;
	font-weight: normal;
}

#contentThree .gutter3 {
	padding: 25px;
	background: url(../images/content-bg.gif) repeat-x top left;
}

#sidebar {
	float: right;
	width: 30%;
}

#sidebar .gutter, sidebar-2 .gutter {
	padding: 15px;
}

#leftsidebar {
	float: left;
	width: 27%;
}

#leftsidebar .gutter, sidebar-3 .gutter {
	padding: 15px;
}

