.col {
	float: left;
	padding: 10px 0 0 15px;
	width: 70.5em;
}

.col:first-child {
	padding: 1px 0px 0 0;
}

.col h2 {
	float: left;
	font: bold 18px Arial, Helvetica, sans-serif;
	font-variant: small-caps;
	margin: 0 0 20px 0;
	padding: 9px 0 0 19px;
	width: 70.5em;
}

.col ul {
	list-style: none;
	margin: 0;
	padding: 0;
}

.col li {
	border-bottom: 1px dotted #25c3ba;
	border-top: 1px dotted #25c3ba;
	float: left;
	margin: 3px 0;
	padding: 0;
}

.col li a {
	display: block;
	float: left;
	font: normal 12px Verdana, Geneva, sans-serif;
	padding: 10px 60px 19px 19px;
	text-decoration: none;
}

.col li:hover {
	background: #5fa4c5;
}

